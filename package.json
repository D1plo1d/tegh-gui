{
  "author": "Rob Gilson",
  "main": "_public/index.html",
  "name": "tegh-gui",
  "node-main": "index.js",
  "description": "A GUI for Tegh",
  "version": "0.0.0",
  "scripts": {
    "start": "brunch watch --server",
    "build": "brunch build --optimize",
    "test": "brunch test"
  },
  "window": {
    "title": "node-webkit demo",
    "toolbar": false,
    "frame": true,
    "position": "center",
    "width": 1280,
    "height": 700,
    "min_width": 1280,
    "min_height": 700
  },
  "chromium-args": "--disable-web-security --auto-ssl-client-auth",

  "dependencies": {
    "coffee-script": "*",
    "nodewebkit": "*",
    "tegh-client-node": "git+https://github.com/tegh/tegh-client-node.git",
    "gaze": "*",
    "lodash": "*",

    "sugar": "*",

    "jade-brunch": "~1.7",
    "static-jade-brunch": "~1.7",
    "javascript-brunch": "~1.7",
    "coffee-script-brunch": "~1.7",
    "css-brunch": "~1.7",
    "sass-brunch": "~1.7",
    "less-brunch": "~1.7",
    "uglify-js-brunch": "~1.7",
    "clean-css-brunch": "~1.7",
    "stylus-brunch": "~1.8"
  },
  "devDependencies": {}
}
