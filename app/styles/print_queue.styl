#print_queue.active
  display flex
  flex-flow column
  flex 1 1 auto

  td, th
    width: (70/6)%
    &:first-child
      width: 30%
    // &:last-child
    //   width: 15%

  table
    margin-bottom: 0px

  .headers-table
    flex 0 0 auto
    padding-right 15px
    border-bottom 2px solid $gray-lighter
    box-sizing border-box
    border-spacing 0px

    table, tr, th
      border: none

  .data-table
    flex 1 1 auto
    overflow-y scroll

    td
      line-height: 35px

    tr:first-child td
      border-top: 0px

    tr.drag-over-top td
      border-top 1px dashed black

    tr.table-drag-over-bottom td
      border-bottom 1px dashed black

  .drag-handle
    margin-right: 10px
    cursor: -webkit-grab

  .big-not-found.centered
    position absolute
    top 50%
    left 0px
    width 100%
    margin-top -30px

  .delete-part
    float right
    line-height 35px
    color $gray
    cursor pointer
    margin-right 10px

    &:hover
      color $brand-danger

  .part-actions .btn
    float right
    margin-right 10px

  .quality-not-applicable
    margin-left 14px

  td input
    width 60px

/* Prevent the text contents of draggable elements from being selectable. */
[draggable]
  user-select none
