#print_queue.active
  display flex
  flex-flow column
  flex 1 1 auto

  td, th
    width: (60/4)%
    &:first-child
      width: 40%

  table
    margin-bottom: 0px

  .headers-table
    flex 0 0 auto

  .data-table
    flex 1 1 auto
    overflow-y scroll

    tr:first-child td
      border-top: 0px

  .drag-handle
    margin-right: 10px
    cursor: -webkit-grab

  .big-not-found.centered
    position absolute
    top 50%
    left 0px
    width 100%
    margin-top -30px