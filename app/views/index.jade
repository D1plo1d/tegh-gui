extends layout
block content

  .tab-content

    #print_queue.tab-pane
      table.table.table-striped
        thead
          tr
            th
            th
            th Qty
        tbody
          tr(ng-repeat="job in jobs()")
            td {{job.name}} {{job}}
            td {{job.quality}}
            td {{job.qty}}


    #manual_ctrl.tab-pane.active
      include ./ctrl_panel

